!urxvt color scheme:
URxvt*background: #121212
URxvt*foreground: #ffffff

URxvt.transparent: false
URxvt.tintColor: white
URxvt.shading: 100

!urxvt fonts:
URxvt*font: xft: InconsolataForPowerline Nerd Font:style=Medium
URxvt*boldFont: xft: InconsolataForPowerline Nerd Font:bold
URxvt*skipBuiltinGlyphs: false

URxvt*perl-ext-common: default,matcher
URxvt.url-launcher: google-chrome-stable
URxvt.matcher.button: 1

URxvt*scrollBar: false
URxvt*cursorBlink: false
URxvt*cursorUnderline: false
URxvt*cursorColor: gray

URxvt*keysym.C-minus: command:\033]50;xft:InconsolataForPowerline Nerd Font:pixelsize=16\007
URxvt*keysym.C-equal: command:\033]50;xft:InconsolataForPowerline Nerd Font:pixelsize=20\007

!black
*color0:  #121212
*color8:  #5e5e5e
!red
*color1:  #db3f62
*color9:  #eb4509
!green
*color2:  #467056
*color10: #4A844C
!yellow
*color3:  #FF7E4A
*color11: #fff718
!blue
*color4:  #7991e5
*color12: #83a598
!magenta
*color5:  #AF87D7
*color13: #AF87D7
!cyan
*color6:  #689d6a
*color14: #8ec07c
!white
*color7:  #cccccc
*color15: #ffffff
